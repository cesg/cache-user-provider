{
    "name": "cesg/cache-user-provider",
    "description": "eloquent user provider",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.6",
        "mockery/mockery": "^1.3",
        "orchestra/testbench": "^4.5|^5.0|^6.0",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "license": "MIT",
    "authors": [{
        "name": "Cristian SG",
        "email": "cesgbla@gmail.com"
    }],

    "autoload": {
        "psr-4": {
            "Cesg\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cesg\\Auth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cesg\\Auth\\CacheUserServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
